def soma_pares(numeros: list, alvo: int) -> bool:
    for x in numeros:
        for y in numeros:
            if x + y == alvo:
                print('As somas que chegam no alvo são: ',str(x), '+' ,str(y), '=', alvo)
                return True

    print('Não existem somas que chegam ao alvo')
    return False



print(soma_pares([1,2,3,4],5))
