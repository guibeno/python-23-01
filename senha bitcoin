import zipfile

NOME_ARQUIVO = "senha_bitcoin (5).zip"
zip_file = zipfile.ZipFile(NOME_ARQUIVO, 'r')


def extrair_arquivo_zip_com_senha(senha):
    try:
        zip_file.extractall(path='../../Pictures', pwd=senha.encode('utf-8'))
        return True
    except:
        pass
    return False


