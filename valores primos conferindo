def eh_primo(numero: int) -> bool:
    if numero <= 1:
        return False
    for i in range(2, numero):
        if numero % i == 0:
            return False
    return True

def lista_primos(numero: int) -> list:
    lista_primos = []
    for i in range(num):
        if eh_primo(i):
            lista_primos.append(i)
    return lista_primos

num = 1000
print(lista_primos(num))
